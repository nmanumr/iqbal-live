{
  "include": [
    ".react-router/types/**/*",
    "src/**/*",
    "src/**/.server/**/*",
    "src/**/.client/**/*",
    "workers/**/*"
  ],
  "compilerOptions": {
    "composite": true,
    "strict": true,
    "lib": ["DOM", "DOM.Iterable", "ES2022"],
    "types": ["@cloudflare/workers-types", "vite/client"],
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "baseUrl": ".",
    "rootDirs": [".", "./.react-router/types"],
    "paths": {
      "~/*": ["./src/*"]
    },
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}