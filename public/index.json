    "name": "جاوید نامہ",
    "year": "",
    "language": "",
    "sections": [
      {
        "id": "01",
        "name": "جاوید نامہ",
        "poems": [
          { "id": "01", "name": "دیباچہ" },
          { "id": "02", "name": "مناجات" },
          { "id": "03", "name": "تمہید آسمانی نخستین روز آفرینش ٓنکوہش" },
          { "id": "04", "name": "نغمہ ملائک" },
          { "id": "05", "name": "تمہید زمینی آشکارا می شود روح حضرت رومی" },
          { "id": "06", "name": "زروان کہ روح زمان و مکان است مسافر را" },
          { "id": "07", "name": "زمزمۂ انجم" }
        ]
      },
      {
        "id": "02",
        "name": "فلک قمر",
        "poems": [
          { "id": "01", "name": "فلک قمر" },
          {
            "id": "02",
            "name": "عارف ہندی کہ بہ یکی از غار ہای قمر خلوت گرفتہ"
          },
          { "id": "03", "name": "نہ تا سخن از عارف ہندی" },
          { "id": "04", "name": "جلوۂ سروش" },
          { "id": "05", "name": "نواے سروش" },
          { "id": "06", "name": "حرکت بہ وادی یرغمید کہ ملائکہ" },
          { "id": "07", "name": "طاسین گوتم تو بہ آوردن زن رقاصہ عشوہ فروش" },
          { "id": "08", "name": "طاسین زرتشت آزمایش کردن اہرمن زرتشت را" },
          { "id": "09", "name": "طاسین مسیح رویای حکیم طالسطایی" },
          { "id": "10", "name": "طاسین محمد نوحۂ ابوجہل در حرم کعبہ" }
        ]
      },
      {
        "id": "03",
        "name": "فلک عطارد",
        "poems": [
          {
            "id": "01",
            "name": "زیارت ارواح جمال الدین افغانی و سعید حلیم پاشا"
          },
          { "id": "02", "name": "دین و وطن" },
          { "id": "03", "name": "اشتراک و ملوکیت" },
          { "id": "04", "name": "شرق و غرب" },
          { "id": "05", "name": "محکمات عالم قرا نے: خلافت آدم" },
          { "id": "06", "name": "حکومت الھی" },
          { "id": "07", "name": "ارض ملک خداست" },
          { "id": "08", "name": "حکمت خیرکثیر است" },
          { "id": "09", "name": "پیغام افغانی با ملت روسیہ" },
          { "id": "10", "name": "غزل زندہ رود" }
        ]
      },
      {
        "id": "04",
        "name": "فلک زہرہ",
        "poems": [
          { "id": "01", "name": "فلک زہرہ" },
          { "id": "02", "name": "مجلس خدایان اقوام قدیم" },
          { "id": "03", "name": "نغمۂ بعل" },
          { "id": "04", "name": "ارواح فرعون و کشنر را" },
          { "id": "05", "name": "نمودار شدن درویش سودانی" }
        ]
      },
      {
        "id": "05",
        "name": "فلک مریخ",
        "poems": [
          { "id": "01", "name": "اہل مریخ" },
          { "id": "02", "name": "برآمدن انجم شناس مریخی از رصدگاہ" },
          { "id": "03", "name": "گردش در شہر مرغدین" },
          { "id": "04", "name": "احوال دوشیزۂ مریخ کہ دعوی رسالت کردہ" },
          { "id": "05", "name": "تذکیر نبیۂ مریخ" }
        ]
      },
      {
        "id": "06",
        "name": "فلک مشتری",
        "poems": [
          { "id": "01", "name": "ارواح جلیلۂ حلاج و غالب و قرةالعین طاہرہ" },
          { "id": "02", "name": "نوای حلاج" },
          { "id": "03", "name": "نوای غالب" },
          { "id": "04", "name": "نوای طاہرہ" },
          {
            "id": "05",
            "name": "زندہ رود مشکلات خود را پیش ارواح بزرگ میگوید"
          },
          { "id": "06", "name": "نمودار شدن خواجۂ اہل فراق ابلیس" },
          { "id": "07", "name": "نالۂ ابلیس" }
        ]
      },
      {
        "id": "07",
        "name": "فلک زحل",
        "poems": [
          { "id": "01", "name": "ارواح رذیلہ کہ با ملک و ملت غداری کردہ" },
          { "id": "02", "name": "قلزم خونین" },
          { "id": "03", "name": "آشکارا می شود روح ہندوستان" },
          { "id": "04", "name": "روح ہندوستان نالہ و فریاد می کند" },
          { "id": "05", "name": "فریاد یکی از زورق نشینان قلزم خونین" }
        ]
      },
      {
        "id": "08",
        "name": "آن سوی افلاک",
        "poems": [
          { "id": "01", "name": "مقام حکیم آلمانی نیچہ" },
          { "id": "02", "name": "حرکت بجنت الفردوس" },
          { "id": "03", "name": "قصر شرف النسا" },
          {
            "id": "04",
            "name": "زیارت حضرت سید علی ھمدانی و ملا طاہر غنی کشمیری"
          },
          { "id": "05", "name": "در حضور شاہ ہمدان" },
          { "id": "06", "name": "صحبت با شاعر ہندی برتری ہری" },
          {
            "id": "07",
            "name": "حرکت بہ کاخ سلاطین مشرق نادر ابدالی سلطان شہید"
          },
          { "id": "08", "name": "روح ناصر خسرو علوی و غزلی مستانہ سراییدہ" },
          { "id": "09", "name": "پیغام سلطان شہید بہ رود کاویری" },
          { "id": "10", "name": "زندہ رود رخصت می شود از فردوس برین" },
          { "id": "11", "name": "غزل زندہ رود" },
          { "id": "12", "name": "حضور" },
          { "id": "13", "name": "خطاب بہ جاوید سخنی بہ نژاد نو " }
        ]
      }
    ]
  },
