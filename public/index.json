          { "id": "06", "name": "لا الہ الا اﷲ" },
          { "id": "07", "name": "فقر" },
          { "id": "08", "name": "مرد حر" },
          { "id": "09", "name": "در اسرار شریعت" },
          { "id": "10", "name": "اشکے چند بر افتراق ہندیان" },
          { "id": "11", "name": "سیاسیات حاضرہ" },
          { "id": "12", "name": "حرفی چند با امت عربیہ" },
          { "id": "13", "name": "پس چہ باید کرد اے اقوام شرق" },
          { "id": "14", "name": "در حضور رسالت مآب" },
          { "id": "15", "name": "خطاب بہ اقوام سرحد" },
          { "id": "16", "name": "سافر وارد میشود بہ شہر کابل" },
          { "id": "17", "name": "بر مزار شہنشاہ بابر خلد آشیانی" },
          { "id": "18", "name": "سفر بہ غزنی و زیارت مزار حکیم سنائی" },
          { "id": "19", "name": "روح حکیم سنائی از بہشت برین جواب میدہد" },
          { "id": "20", "name": "بر مزار سلطان محمود علیہ الرحمہ" },
          { "id": "21", "name": "مناجات مرد شوریدہ در ویرانۂ غزنے" },
          { "id": "22", "name": "قندہار و زیارت خرقہ مبارک" },
          { "id": "23", "name": "بر مزار حضرت احمد شاہ بابا علیہ الرحمہ" },
          { "id": "24", "name": "خطاب بہ پادشاہ اسلام اعلیحضرت ظاہر شاہ" }
        ]
      }
    ]
